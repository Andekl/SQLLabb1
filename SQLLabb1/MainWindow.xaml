<Window x:Class="SQLLabb1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SQLLabb1"
        mc:Ignorable="d"
        Title="MainWindow" Height="517.308" Width="730.128">
    <Grid>
        <ListBox x:Name="AuthorListBox" HorizontalAlignment="Left" Height="173" Margin="76,57,0,0" VerticalAlignment="Top" Width="240" SelectionChanged="AuthorListBox_SelectionChanged" IsSynchronizedWithCurrentItem="False"/>
        <ListBox x:Name="BookListBox" HorizontalAlignment="Left" Height="173" Margin="409,57,0,0" VerticalAlignment="Top" Width="240" SelectionChanged="BookListBox_SelectionChanged"/>
        <TextBox x:Name="AuthorTextBox" HorizontalAlignment="Left" Height="95" Margin="76,242,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="240"/>
        <TextBox x:Name="BookTextBox" HorizontalAlignment="Left" Height="95" Margin="409,242,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="240"/>
        <Button x:Name="SaveChangesButton" Content="SAVE CHANGES" HorizontalAlignment="Left" Margin="104,382,0,0" VerticalAlignment="Top" Width="120" Height="60"/>
        <Button x:Name="AddElementButton" Content="ADD ELEMENT" HorizontalAlignment="Left" Margin="298,382,0,0" VerticalAlignment="Top" Width="120" Height="60"/>
        <Button x:Name="DeleteElementButton" Content="DELETE ELEMENT" HorizontalAlignment="Left" Margin="488,382,0,0" VerticalAlignment="Top" Width="120" Height="60"/>

    </Grid>
</Window>
